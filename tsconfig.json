{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "types": [
      "node"
    ],
    "paths": {
      "unocss-preset-component": [
        "./src/index.ts"
      ]
    }
  },
  "exclude": [
    "**/dist/**",
    "**/node_modules/**"
  ],
}